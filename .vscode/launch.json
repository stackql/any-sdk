{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "inputs": [
        {
            "type": "pickString",
            "id": "registryRootDir",
            "description": "Registry Configuration",
            "options": [
                "${workspaceFolder}/test/registry",
                "${workspaceFolder}/../stackql-provider-registry/providers",
            ],
            "default": "${workspaceFolder}/test/registry"
        },
        {
            "type": "pickString",
            "id": "providerFilePath",
            "description": "Auth Input arg String",
            "default": "${workspaceFolder}/test/registry/src/aws/v0.1.0/provider.yaml",
            "options": [
                "${workspaceFolder}/test/registry/src/aws/v0.1.0/provider.yaml",
                "${workspaceFolder}/../stackql-provider-registry/providers/src/aws/v00.00.00000/provider.yaml",
                "${workspaceFolder}/../stackql-provider-registry/providers/src/k8s/v00.00.00000/provider.yaml",
                "${workspaceFolder}/../stackql-provider-registry/providers/src/digitalocean/v00.00.00000/provider.yaml"
            ]
        }
    ],
    "configurations": [
        {
            "type": "go",
            "request": "launch",
            "name": "Run standalone file",
            "mode": "auto",
            "program": "${file}",
            "envFile": "${workspaceFolder}/.vscode/.env",
        },
        {
            "type": "go",
            "request": "launch",
            "name": "CLI local: rsa openssl create",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/interrogate",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "args": [
                "query",
                "--svc-file-path=${workspaceFolder}/test/registry/src/local_openssl/v0.1.0/services/keys.yaml",
                "--prov-file-path=${workspaceFolder}/test/registry/src/local_openssl/v0.1.0/provider.yaml",
                "--resource",
                "rsa",
                "--method",
                "create_key_pair",
                "--parameters",
                "{ \"config_file\":   \"${workspaceFolder}/test/openssl/openssl.cnf\", \"key_out_file\": \"${workspaceFolder}/test/tmp/key.pem\", \"cert_out_file\": \"${workspaceFolder}/test/tmp/cert.pem\", \"days\": 90}"
            ]
        },
        {
            "type": "go",
            "request": "launch",
            "name": "CLI aws xml: ec2 volumes describe",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/interrogate",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "args": [
                "query",
                "--svc-file-path=${workspaceFolder}/test/registry-simple/src/aws/v0.1.0/services/ec2.yaml",
                "--prov-file-path=${workspaceFolder}/test/registry-simple/src/aws/v0.1.0/provider.yaml",
                "--resource",
                "volumes_presented",
                "--method",
                "describeVolumes",
                "--parameters",
                "{ \"region\": \"ap-southeast-2\"}"
            ]
        },
        {
            "type": "go",
            "request": "launch",
            "name": "CLI aws xml: s3 abac put",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/interrogate",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "args": [
                "query",
                "--svc-file-path=${workspaceFolder}/test/registry-simple/src/aws/v0.1.0/services/s3.yaml",
                "--prov-file-path=${workspaceFolder}/test/registry-simple/src/aws/v0.1.0/provider.yaml",
                "--resource",
                "bucket_abac",
                "--method",
                "put_bucket_abac",
                "--parameters",
                "{ \"region\": \"ap-southeast-2\", \"Bucket\": \"stackql-trial-bucket-02\", \"Status\": \"Disabled\", \"line_items\": [ { \"attribute_name\": \"stackql:createdBy\", \"attribute_value\": \"stackql-user\", \"attribute_value_type\": \"string\" } ] }"
            ]
        },
        {
            "type": "go",
            "request": "launch",
            "name": "CLI local: x509 openssl describe",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/interrogate",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "args": [
                "query",
                "--svc-file-path=${workspaceFolder}/test/registry/src/local_openssl/v0.1.0/services/keys.yaml",
                "--prov-file-path=${workspaceFolder}/test/registry/src/local_openssl/v0.1.0/provider.yaml",
                "--resource",
                "x509",
                "--method",
                "describe_certificate",
                "--parameters",
                "{ \"cert_file\": \"${workspaceFolder}/test/tmp/cert.pem\"}"
            ]
        },
        {
            "type": "go",
            "request": "launch",
            "name": "AOT provider analysis",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/interrogate",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "args": [
                "aot",
                "${input:registryRootDir}",
                "${input:providerFilePath}",
            ]
        }
    ]
}